#<=**>    \lesseqqgtr
#>=**<    \gtreqqless
#<>/   \nlessgtr
#></   \ngtrless
#<~/   \nlesssim
#<~/*    \lnsim
#<~~/    \nlessapprox
#<~~/*    \lnapprox
#<*~/    \nprecsim
#<*~/*    \precnsim
#<*~~/    \nprecapprox
#<*~~/*    \precnapprox
#>~/   \ngtrsim
#>~/*    \gnsim
#>~~/    \ngtrapprox
#>~~/*    \gnapprox
#>*~/   \nsuccsim
#>*~/*    \succnsim
#>*~~/    \nsuccapprox
#>*~~/*    \succnapprox
#<=**/    \nleqq
#>=**/    \ngeqq
#<=*>/    \nlesseqgtr
#>=*</    \ngtreqless
#<=**>/    \nlesseqqgtr
#>=**</    \ngtreqqless
#<=*/*    \lneq
#<=**/*    \lneqq
#<=**/**    \lvertneqq
#>=*/*    \gneq
#>=**/*    \gneqq
#>=**/**    \gvertneqq
#[[                *    \llbracket
#]]                *    \rrbracket
#EE                a    \amalg
#EE                d    \partial
#EE                p    \wp
#EE                n    \cap
#EE                u    \cup
#EE                w    \wedge
#\wedge            *    \curlywedge
#\curlywedge       *    \curlywedgeuparrow
#\curlywedgeuparrow   * \curlywedgedownarrow
#\curlywedgedownarrow * \wedges
#EE                v    \vee
#EE                x    \times

# <***/    \nsqsubset
# <***=    \sqsubseteq
# <***=/    \nsqsubseteq
# >*=/**    \varsupsetneq
# >*=*/*    \supsetneqq
# >*=*/**    \varsupsetneqq
# >***    \sqsupset
# >***/    \nsqsupset
# >***=    \sqsupseteq
# >***=/    \nsqsupseteq

# >****    \rangle
# \rangle  *     \rightslice

# EE                l    \bigl
# EE                m    \bigm
# EE                r    \bigr
# EE                @    \bigop
# EE                L    \int
